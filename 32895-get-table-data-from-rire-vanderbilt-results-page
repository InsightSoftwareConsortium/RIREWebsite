  $("#add_tags").multicomplete({
    data_source: { url: "/matlabcentral/fileexchange/ui/tags/auto_suggest" }
  });

  $(".add-tags-link").click(function() {
    $("#add_tags_form").show();
    $("#add_tags").focus();
    return false;
  });

  $("#add_tags_form .cancel_button").click(function() {
    $("#add_tags").val('').blur();
    $("#add_tags_form").hide();
  });

  $("#add_new_tags_form").on('ajax:complete', function(){
    $('.multicomplete > .multicomplete-selected-item').remove()
    $("#add_tags").val('').blur();
    $("#add_tags_form").hide();
  });
</script>


